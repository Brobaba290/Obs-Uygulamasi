@model IEnumerable<ObsUygulaması.Models.Duyuru>
@{
    ViewBag.Title = "Öğrenci Duyuru Listele";
}

<h2>Duyurular</h2>


<table class="table">
    <tr>
        <th>
            Duyuru Adı
        </th>
        <th>
            Açıklama
        </th>
        <th>
            Tarih
        </th>

        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Baslik)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Icerik)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Tarih)
            </td>
            
        </tr>
    }

</table>
<div>
    @Html.ActionLink("Önceki Sayfa", "OgrenciGiris", "Home")
</div>