    @model IEnumerable<ObsUygulaması.Models.Duyuru>

    <!--END OF team SECTION-->
    <section class="wrapper" id="services">
        <div class="container">
            <br>
            <center><h4 style="background-color:rgb(205,0,0)">DUYURULAR</h4></center>
            <div class="info clearfix">


                <table class="table">
                    <tr>
                        <th>
                            Duyuru Adı
                        </th>
                        <th>
                            Açıklama
                        </th>
                        <th>
                            Tarih
                        </th>

                    </tr>

                    @foreach (var item in Model)
                    {
                        <tr>
                            <td>
                                @Html.DisplayFor(modelItem => item.Baslik)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.Icerik)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.Tarih)
                            </td>

                        </tr>
                    }

                </table>
            </div>


        </div>
    </section>



